{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "cleanUrls": true,
  "functions": {
    "dist": {
      "includeFiles": "dist/**/*"
    },
    "images": {
      "includeFiles": "/src/images/**/*"
    },
    "fonts": {
      "includeFiles": "/dist/fonts/**/*"
    },
    "sw.js": {
      "includeFiles": "/dist/js/sw.js"
    },
    "manifest.json": {
      "includeFiles": "/manifest.json"
    }
  }
}